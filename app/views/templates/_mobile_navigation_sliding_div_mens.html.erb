<% mens_categories = {'All': '/men/all', 'Accessories': '/men/accessorie', 'Formal': '/men/formal', 'Lifestyle': '/men/lifestyle', 'Longs': '/men/long' , 'Shoes': '/men/shoe' , 'Shorts & Swimwear': '/men/short_swimwear', 'Sunglasses & Watches': '/men/sunglass_watch' , 'Tops': '/men/top', 'SHOP THE LOOK': '/men/shop_the_look' }; %>


<div class="hamburger-slider">
  <div class="container">
    <div class="row">
      <ul>
        <li><b>Shop by</b></li>
        <% mens_categories.each do |keys, values| %>
          <li><%= link_to keys, values %></li>
        <% end %>
        <div class="bottom_list_tags">
          <li><%= link_to "ALL BRANDS", men_brand_path %></li>
          <li><%= link_to "ON SALE", site_sale_mens_path %></li>
          <li><%= link_to "SEARCH", men_brand_mobile_men_search_path %></li>
        </div>
        <div class="about_contact_list_tags">
          <li><%= link_to "About", men_about_path%>&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp<%= link_to "Contact", men_contact_path %></li>
        </div>
      </ul>
    </div>
  </div>
</div>