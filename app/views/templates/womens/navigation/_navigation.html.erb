<% womens_categories = {'All': '/women/all', 'Accessories': '/women/accessorie', 'Dresses': '/women/dress','Jeans Trousers & Leggings': '/women/jean_legging' , 'Home & Lifestyle': '/women/lifestyle', 'Shoes': '/women/shoe' , 'Shorts & Skirts': '/women/short_skirt', 'Sunglasses & Watches': '/women/sunglass_watch' , 'Swim & Beachwear': '/women/swim_beachwear', 'Tops': '/women/top', 'SHOP THE LOOK': '/women/shop_the_look' }; %>

<% womens_brands = {'Alice McCall': '/women_brand/alice_mccall' , 'Auguste': '/women_brand/auguste','Bronze Snake': '/women_brand/bronze_snake','Cooper St': '/women_brand/coopers_st','Lorna Jane': '/women_brand/lorna_jane', 'Mura Boutique': '/women_brand/mura','New Balance': '/women_brand/new_balance','Samantha Wills': '/women_brand/samantha_wills', 'Seafolly': '/women_brand/sea_folly', 'Showpo': '/women_brand/showpo', 'Sunday Somewhere': '/women_brand/sunday_somewhere', 'Tony Bianco': '/women_brand/tony_bianco'} %>

<div class="sticky-nav-mens">
  <h1>InstaBoss</h1>
  <div class="all-nav-link">
    <%= link_to "MENS", men_path %>
    <u><%= link_to "WOMENS", women_path %></u>
    <div class="item_nav_top">
      <%= link_to "SHOP",  class: "shop_by" %>
      <div class="item_drop_shop_by" id="my_drop_down">
        <% womens_categories.each do |keys, values| %>
          <%= link_to keys, values %>
        <% end %>
      </div>
      <div class="arrow-down-shop"></div>
    </div>
    <%= link_to "SHOP THE LOOK", women_shop_the_look_path %>
    <%= link_to "SALE", site_sale_womens_path %>
    <div class="item_nav_top_brand">
      <%= link_to "BRANDS", women_brand_path, class: "shop_by_brand" %>
      <div class="item_drop_shop_by_brand" id="my_drop_down_brand">
        <% womens_brands.each do |keys, values| %>
          <%= link_to keys, values %>
        <% end %>
        <%= link_to "ALL BRANDS", women_brand_path %>
      </div>
      <div class="arrow-down-brand"></div>
    </div>
    <%= link_to "ABOUT", women_about_path %>
    <%= link_to "CONTACT", women_contact_path %>
  </div>
</div>
<div class="banner-nav-space"></div>